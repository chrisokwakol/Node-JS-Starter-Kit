{
  "name": "javascript-development-environment",
  "version": "1.0.0",
  "description": "Node js development starter kit",
  "scripts": {
    "dev": "nodemon --exec babel-node server.js",
    "lint": "esw webpack.config.* public server.js --color",
    "lint:watch": "npm run lint -- --watch",
    "test": "mocha --reporter progress tests/testSetup.js \"public/**/*.test.js\"", 
    "start": "npm-run-all --parallel dev lint:watch test:watch",
    "test:watch": "npm run test -- --watch"
  },
  "author": "Chris",
  "license": "MIT",
  "dependencies": {
    "whatwg-fetch": "1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.3",
    "babel-cli": "6.16.0",
    "babel-loader": "^8.1.0",
    "babel-preset-latest": "6.16.0",
    "babel-register": "6.16.3",
    "chai": "3.5.0",
    "chalk": "1.1.3",
    "cheerio": "0.22.0",
    "compression": "^1.7.4",
    "cross-env": "3.1.3",
    "css-loader": "^3.6.0",
    "eslint": "^7.3.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-watch": "^7.0.0",
    "express": "^4.17.1",
    "html-webpack-plugin": "^4.3.0",
    "jsdom": "9.8.0",
    "json-schema-faker": "^0.4.7",
    "json-server": "^0.16.1",
    "mini-css-extract-plugin": "^0.9.0",
    "mocha": "^8.0.1",
    "nock": "^13.0.0",
    "nodemon": "^2.0.4",
    "npm-run-all": "3.1.1",
    "numeral": "1.5.3",
    "open": "^7.0.4",
    "rimraf": "2.5.4",
    "style-loader": "0.13.1",
    "surge": "0.20.4",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-hot-middleware": "2.13.0",
    "webpack-md5-hash": "0.0.5"
  }
}
